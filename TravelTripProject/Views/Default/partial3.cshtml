@using TravelTripProject.Models.Siniflar
@model List<Blog>
 
<div class="padding">
    <h4>En Popüler 10 Blog</h4>
    <ul class="list list-pad">
        @{ int count = 0;}
        @foreach (var item in Model)
        {
            ++count;
            <li class="list-bg"><span>@count</span><a href="/Blog/BlogDetay/@item.ID">@item.Baslik</a></li>
        }
    </ul>
</div>
